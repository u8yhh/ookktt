# 顶格注释内容在sed处理时将被剔除，不会出现在最后的配置文件中
# 在heroku上fast_open必须为false
{
    "log": {
        "loglevel": "warning"
    },
    "routing": {
        "domainStrategy": "AsIs",
        "rules": [
            {
                "type": "field",
                "ip": [
                    "geoip:private"
                ],
                "outboundTag": "block"
            }
        ]
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 12345,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "${Vless_UUID}"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": false,
                    "path": "${Vless_Path}"
                }
            }
        },
        {
            "listen": "0.0.0.0",
            "port": 12346,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                    "id": "${Vmess_UUID}",
                    "level": 0,
                    "alterId": 0,
                    "email": "love@xray.com"
                    }
                ],
                "disableInsecureEncryption": false
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": false,
                    "path": "${Vmess_Path}"
                }
            }
        },
        {
            "listen": "0.0.0.0",
            "port": 12347,
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password":"${Trojan_UUID}",
                        "level": 0,
                        "email": "love@v2fly.org"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "allowInsecure": true,
                "wsSettings": {
                    "acceptProxyProtocol": false,
                  "path": "${Trojan_Path}"
                }
            }
        },
        {
            "listen": "0.0.0.0",
            "port": 12348,
            "protocol": "shadowsocks",
            "settings": {
                "email": "love@v2fly.org",
                "method": "${ParameterSSENCYPT}",
                "password":"${SS_UUID}",
                "network": "tcp,udp",
                "ivCheck": true
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": false,
                    "path": "${SS_Path}"
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "tag": "block"
        }
    ]
}
